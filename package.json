    "json-loader": "^0.5.7",
    "next": "13.1.6",
    "next-remove-imports": "^1.0.8",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.43.1",
