{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier", "unused-imports"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "warn",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars":  "error"
  },
  "ignorePatterns": ["node_modules/"]
}
